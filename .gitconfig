[alias]
  co = checkout
  ci = commit
  ca = commit --amend
  st = status
  br = branch
  pu = push
  po = push --set-upstream origin \"$CURRENT_BRANCH\"
  hs = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  hd = log -p --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  gh = log --graph --decorate --oneline --all
